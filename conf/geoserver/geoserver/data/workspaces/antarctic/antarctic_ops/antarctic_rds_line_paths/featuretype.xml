      <virtualTable>
        <name>antarctic_rds_line_paths</name>
        <sql>select segs.*, seasons.name from rds_segments as segs INNER JOIN rds_seasons as seasons ON segs.season_id=seasons.id where seasons.location_id=%location_id% and seasons.name IN (%season_name%) and seasons.public IN (%status%) and segs.name &gt; %start_seg% and segs.name &lt; %stop_seg%</sql>
